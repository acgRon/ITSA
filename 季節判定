#include <stdio.h>

int main() {
    int month;

    // 輸入月份
    //printf("請輸入月份（1-12）: ");
    scanf("%d", &month);

    // 判斷季節
    switch (month) {
        case 1:
        case 2:
        case 12:
            printf("Winter\n");
            break;
        case 3 ...5:
            printf("Spring\n");
            break;
        case 6 ...8:
            printf("Summer\n");
            break;
        case 9 ...11:
            printf("Autumn\n");
            break;
        default:
            break;
    }

    return 0;
}
